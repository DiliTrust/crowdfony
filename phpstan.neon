includes:
	- vendor/phpstan/phpstan-symfony/extension.neon
	- vendor/phpstan/phpstan-doctrine/extension.neon

parameters:
	level: 8
	paths:
		- src
	ignoreErrors: []
	scanDirectories:
		- %rootDir%/../../../migrations
	symfony:
		container_xml_path: %rootDir%/../../../var/cache/dev/App_KernelDevDebugContainer.xml
	excludePaths:
		- %rootDir%/../../../tests
